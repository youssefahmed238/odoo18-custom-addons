    <record id="stock_move_line_view_search_inherit" model="ir.ui.view">
        <field name="name">stock.move.line.search.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.stock_move_line_view_search"/>
        <field name="arch" type="xml">
            <xpath expr="//search/group/filter[@name='by_state']" position="after">
                <filter string='Vendor' name="vendor" context="{'group_by': 'vendor'}"/>
            </xpath>

            <xpath expr="//search" position="inside">
                <field name="vendor"/>
            </xpath>
        </field>
    </record>


    <record id="view_move_line_list_inherit" model="ir.ui.view">
        <field name="name">stock.move.line.list.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='lot_id']" position="after">
                <field name="vendor"/>
            </xpath>
        </field>
    </record>

    <record id="view_product_product_list_inherit_total_value" model="ir.ui.view">
        <field name="name">product.product.list.inherit.total.value</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="stock.view_stock_product_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='qty_available']" position="after">
                <field name="total_value"/>
            </xpath>
        </field>
    </record>





